{
  "name": "node-api",
  "description": "node api",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "keywords": [],
  "author": "john paul onte <jnpl.onte@gmail.com>",
  "license": "MIT",
  "quokka": {
    "babel": true
  },
  "scripts": {
    "build:production": "concurrently \"tsc --project ./src/tsconfig.app.json\" \"npm run generate:config\"",
    "build:development": "concurrently \"tsc --project ./tsconfig.json\" \"npm run generate:config\" \"npm run generate:docs\"",
    "generate:config": "copyfiles -u 1 ./src/config/*.json ./dist",
    "generate:docs": "apidoc -i ./src/ -o ./dist/doc/ -f .ts",
    "start": "concurrently \"tsc --watch\" \"nodemon ./dist/index.js\"",
    "start:server": "node ./dist/index.js",
    "test": "concurrently \"tsc\" \"PORT=8883 TEST_DIR='./dist/doc' ARTIFACTS_DIR='./dist/doc' COVERAGE_DIR='./dist/doc' mocha dist/**/*.spec.js --timeout 100000 --exit\"",
    "test:one": "concurrently \"tsc\" \"mocha ./dist/app/v1/core/**/*.spec.js --timeout 100000 --no-coverage --exit\"",
    "lint": "tslint -c ./tslint.json './src/**/*.ts' -e './src/**/*.spec.ts'",
    "migrate": "sequelize db:migrate  --env local --config ./src/config/database-config.json --migrations-source-path ./src/migrations --migrations-compiled-path ./dist/migrations",
    "seed": "sequelize db:seed:all --env local --config ./src/config/database-config.json --seeders-path ./dist/seeders",
    "seed:revert": "sequelize db:seed:undo:all --env local --config ./src/config/database-config.json --seeders-path ./dist/seeders",
    "create:model": "sequelize model:create --migrations-source-path ./src/migrations --models-path ./src/models --migrations-compiled-path ./dist/migrations --attributes title:string --name",
    "create:seed": "sequelize seed:create --seeders-path ./src/seeders --name",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "copyfiles": "^2.2.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-jwt": "^5.3.3",
    "express-redis-cache": "^1.1.3",
    "fsevents": "^2.1.2",
    "helmet": "^3.22.0",
    "jnpl-helper": "^1.5.16",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/node": "^13.13.0",
    "@types/sequelize": "^4.28.8",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/express-serve-static-core": "^4.17.5",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "apidoc": "^0.20.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "tslint": "^6.1.1"
  }
}
